{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "short_intro": {
      "type": "text"
    },
    "minutes": {
      "type": "integer"
    },
    "intro": {
      "type": "richtext"
    },
    "blog_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::blog-category.blog-category"
    },
    "author": {
      "type": "component",
      "repeatable": false,
      "component": "blog.author"
    },
    "image": {
      "type": "component",
      "repeatable": false,
      "component": "blog.image"
    },
    "paragraphs": {
      "type": "component",
      "repeatable": true,
      "component": "blog.paragraph"
    },
    "production": {
      "type": "boolean",
      "default": true
    },
    "video": {
      "type": "component",
      "repeatable": true,
      "component": "video.video"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    }
  }
}
